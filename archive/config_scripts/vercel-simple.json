{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/simple_index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "functions": {
    "api/simple_index.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  }
}
